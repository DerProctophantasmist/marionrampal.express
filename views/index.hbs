        <div>
            <x-import-google-fonts fonts="googleFonts" > </x-import-google-fonts>
            <x-font-style ng-repeat='section in website.sections.data'  ng-if="section.googleFont != undefined" font="section.googleFont" css-font-class=".special-font" css-selector=".section-\{{section.id}}"></x-font-style> 
            <style type="text/css"> 
                .menuToggle line { 
                    stroke: \{{website.headerColor()}}; 
                    stroke-width: 2; 
                }
                .menuToggle svg {    
                    position: absolute;
                    display: block;
                    /*float: right;*/
                    width: 1.4em;
                    height: 1.4em;
                    right: 0.7em;
                    top: 50%;
                    margin-top: -0.4em;
                }
                .title-banner span:before, .title-banner span:after {
                    content: "";
                    position: absolute;
                    height: 5px;
                    border-top: 2px solid;
                    border-color:  \{{website.headerColor()}}; 
                    top: 50%;
                    width: 100vw;
                }

            </style>

            <!-- Header -->
            <header ng-style="{'background-image':website.headerImage(), 'background-position':(website.sections.topPage.bkgImgPos||''), 'color':website.headerColor()}" id="header" ng-class="{'alt':!website.isMobileLayout()}">

                <!-- Logo -->
                <h3  ng-if="!website.isMobileLayout()" class="title-banner special-font"><span> Marion Rampal â€” \{{website.sections.topPage.section.getname()}}</span></h3>

                <!-- Nav -->
                <nav id="nav" ng-style="{'background-color':website.menuBackground()}">
                    <ul>                        
<!--                        <li ng-controller="LangPickCtrl as vm" class="menuToggle clickable">
                            <a ng-click="vm.changeLocale()"><span ng-class="vm.classShowFlag()"></span>  \{{vm.selectedLocale}}</a>
                        </li>-->
                        <li class="special clickable">
                            <a main-menu class="menuToggle"><span>Menu</span>
                                <svg xmlns="http://www.w3.org/2000/svg" class="menu-icon" xmlns:xlink="http://www.w3.org/1999/xlink" viewBox="0 0 16 16" >

                                <line x1="0" y1="1" x2="16" y2="1" />
                                <line x1="0" y1="7" x2="16" y2="7" />
                                <line x1="0" y1="13" x2="16" y2="13" />
                                </svg>
                            </a>
                        </li>
                    </ul>
                </nav>

            </header>

            <!-- Intro -->
            <section ng-repeat='section in website.sections.data' ng-if="website.displaySection(section.id)" ng-init="sectionIndex = $index" id="\{{section.id}}" ng-class="'section-' + section.id">            
                <x-page ng-repeat='page in section.pages' ng-include="page.template" ng-init="pageIndex = $index"></x-page>
            </section>


            <!-- Footer -->
            <footer id="footer">

                <!-- Icons -->
                <ul class="actions">
                    <li><a href="https://www.facebook.com/pagemarionrampal" class="icon fa-facebook"><span class="label">Facebook</span></a></li>
                    <li><a href="https://soundcloud.com/marion-rampal" class="icon fa-soundcloud"><span class="label">Soundcloud</span></a></li>
                    <li><a href="https://www.youtube.com/channel/UCDYIvvg9blbBUXV702dCWEQ" class="icon fa-youtube"><span class="label">Youtube</span></a></li>
<!--                    <li><a href="#" class="icon fa-pinterest"><span class="label">Pinterest</span></a></li>
                    <li><a href="#" class="icon fa-instagram"><span class="label">Instagram</span></a></li>-->
                </ul>

                <!-- Menu -->
                <ul class="menu">
                    <small><li>&copy; Marion Rampal</li><li>Artwork on Main Blue: Marc Hernandez</li><li>Photos: Martin Sarrazac</li><li>Design and Implementation: Martin Sarrazac with a kickstart from <a href="http://html5up.net">HTML5 UP</a></li></small>
                </ul>

            </footer> 

    </body>
</html>